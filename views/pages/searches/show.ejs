<!DOCTYPE html>
 <html lang="en">
 <head>
     <meta charset="UTF-8">
     <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <link rel="stylesheet" href="/styles/reset.css">
     <link rel="stylesheet" href="/styles/base.css">
     <link rel="stylesheet" href="/styles/layouts.css">
     <link rel="stylesheet" href="/styles/modules.css">
     <title>Document</title>
 </head>
 <body>

    <header>
     <h1>The Book App</h1>
     <nav>
       <h3><a class="a-header" href="/">HOME</a></h3>
       <h3><a class="a-header" href="/searches/new">SEARCH</a></h3>
     </nav>
   </header>

    <main>
     <div class="book-results">
       <h2>Search Results</h2>
       <%books.map(data=>{%>
         <div class="book-grid">
           <ul>
             <li> <img class="book-img" src="<%=data.image_url%>" /> </li>
             <li class="book-title"><%= data.title%></li>
             <li class="book-author"><%= data.authors%></li>
            
               <li class="book-description"><%= data.description %></li>  
                
           </ul>
          
         </div>
         <% });%>
     </div>
   </main>
 </body>
 </html>